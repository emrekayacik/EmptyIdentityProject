@model IEnumerable<Emre.MvcWebUI.Identity.ApplicationUser>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Users</h2>
<hr/>
<div class="container">
    <div class="panel panel-primary">
        <div class="panel-heading">
            Users
        </div>
        <table class="table table-striped">
            <tr>
                <th>Id</th>
                <th>Username</th>
                <th>Email</th>
            </tr>
            @if (Model.Count() == 0)
            {
                <tr>
                    <td colspan="3" class="text-center">
                        Kullanıcı yok
                    </td>
                </tr>
            }
            else
            {
                foreach (var user in Model)
                {
                    <tr>
                        <td>@user.Id</td>
                        <td>@user.UserName</td>
                        <td>@user.Email</td>
                    </tr>

                }
            }
        </table>
    </div>
</div>